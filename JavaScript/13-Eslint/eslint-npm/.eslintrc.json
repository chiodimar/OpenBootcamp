{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent":["error",4],
        "linebreak-style":["error","windows"],
        "semi": ["error", "never"],
        "quotes": ["error", "double"],
        "no-unused-vars":"off"
    }
}
